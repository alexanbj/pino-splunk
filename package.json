    "node-fetch-native": "^1.0",
    "pino-abstract-transport": "^1.0"
  },
  "devDependencies": {
    "pino": "^8",
    "unbuild": "0.9.4",
    "vi-fetch": "^0.8.0",
    "vitest": "0.25.1"
  },
  "types": "./dist/lib.d.ts",
  "unbuild": {
    "entries": [
      "./src/lib"
    ],
    "declaration": true
