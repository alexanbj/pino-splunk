{
  "name": "pino-splunk",
  "version": "0.4.1",
  "description": "Load pino logs into Splunk",
  "type": "module",
  "exports": {
    ".": "./dist/lib.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "lint": "pnpm rome check . && pnpm rome format .",
    "lint:ci": "pnpm rome ci .",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "pino",
    "log",
    "logging",
    "splunk"
  ],
  "files": [
    "dist"
  ],
  "author": "Alexander Bjerkan <alexander.bjerkan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "node-fetch-native": "^1.0",
    "pino-abstract-transport": "^1.0"
  },
  "devDependencies": {
    "pino": "^8",
    "rome": "^11.0.0",
    "unbuild": "1.1.1",
    "vi-fetch": "0.8.0",
    "vitest": "0.28.4"
  },
  "types": "./dist/lib.d.ts",
  "unbuild": {
    "entries": [
      "./src/lib"
    ],
    "declaration": true
  }
}
